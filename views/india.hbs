                            <div class="card-body">
                                <div class="d-sm-flex justify-content-between">
                                    <div class="table-responsive">
                                        <table id="example" class="table table-striped table-bordered" style="width:100%;">
                                            <thead>
                                                <tr>
                                                    <th>State</th>
                                                    <th>Confirmed Cases</th>
                                                    <th>Recovered Cases</th>
                                                    <th>Total Deaths</th>
                                                    <th>Active Cases</th>
                                                    <th>Last Updated</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                {{#each india}}
                                                    <tr>
