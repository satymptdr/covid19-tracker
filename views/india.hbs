                                                </tr>
                                            </thead>
                                            <tbody>
                                                {{#each india}}
                                                    <tr>
                                                        <th style="font-weight: 1000;">{{this.state}}</th>
                                                        <td style="color: red; font-weight: 600">{{this.confirmed}} <p></p><span style="color: red; font-weight: 300">+{{this.deltaconfirmed}}</span></td>
                                                        <td style="color: green; font-weight: 600">{{this.recovered}} <p></p><span style="color: green; font-weight: 300">+{{this.deltarecovered}}</span></td>
                                                        <td style="color: black; font-weight: 600">{{this.deaths}} <p></p><span style="color: black; font-weight: 300">+{{this.deltadeaths}}</span></td>
                                                        <td style="color: blue">{{this.active}}</td>
                                                        <td>{{formatIndiaCasesTime lastupdatedtime 'DD/MM/YYYY HH:mm:ss'}}</td>
                                                    </tr>
                                                {{/each}}
                                            </tbody>
