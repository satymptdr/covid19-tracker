<script>
        $(document).ready(function() {

            if ( $.fn.dataTable.isDataTable( '#example' ) ) {
                table = $('#example').DataTable();
            }
            else {
                table = $('#example').DataTable( {
                    "lengthMenu": [[10, 50, 100, -1], [10, 50, 100, "All"]],
                    "paging":   true,
                    "ordering": true,
                    "info":     true,
                    "order": [[ 9, "desc" ]]
                } );
            }
        } );

    </script>

