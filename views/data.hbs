<div id="main_content">
    <div class="page">
        <div class="section-body mt-3">
            <div class="container-fluid">
                <div class="row clearfix row-deck">
                    <div class="col-lg-12 col-md-12">
                        <div class="row mb-3">
                            <div class="col-lg-12 col-md-12">
                                <a type="button" href="/" class="btn btn-danger mr-3">Home</a>
                                <a type="button" href="/india" class="btn btn-success mr-3">India</a>
                                <a type="button" href="/news" class="btn btn-info">news</a>
                            </div>
                        </div>
                        
                        <div class="card">
                            <div class="card-header bline">
                                <h3 class="card-title">User Logs</h3>
                            </div>
                            <div class="card-body">
                                <div class="d-sm-flex justify-content-between">
                                    <div class="table-responsive">
                                        <table id="example" class="table table-striped table-bordered" style="width:100%;">
                                            <thead>
                                                <tr>
                                                    <th>ID</th>
                                                    <th>IP</th>
                                                    <th>City</th>
                                                    <th>State</th>
                                                    <th>Country</th>
                                                    <th>Postal</th>
                                                    <th>Latitude</th>
                                                    <th>Longitude</th>
                                                    <th>telecome</th>
                                                    <th>Time</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                {{#each user}}
                                                    <tr>
                                                        <th style="font-weight: 1000;">{{this._id}}</th>
                                                        <td>{{this.ip}}</td>
                                                        <td>{{this.city}}</td>
                                                        <td>{{this.state}}</td>
                                                        <td>{{this.country}}</td>
                                                        <td>{{this.postal}}</td>
                                                        <td>{{this.latitude}}</td>
                                                        <td>{{this.longitude}}</td>
                                                        <td>{{this.telecome}}</td>
                                                        <td>{{this.timestamp}}</td>
                                                    </tr>
                                                {{/each}}
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="section-body">
            <footer class="footer">
                <div class="container-fluid">
